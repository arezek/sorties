<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <link rel="icon" type="image/png" sizes="20x20" href="img/logo/logoSortir.png">
        <link rel="stylesheet" href="{{ asset('../public/css/style.css') }}">
        {# 
        <link rel="stylesheet" href="{{ asset('../public/css/normalize.css') }}">
        #}
        <title>{% block title %}Sortie{% endblock %}</title>
    </head>

    <body>
        {# //_______________________________________Header #}
    {% include "main/incl/header.html.twig" %}

    {# //_______________________________________Section #}
    <section>
        {# read and display all flash messages #}
        {% for label, messages in app.flashes %}
            {% for message in messages %}
                <div
                    class="flash-{{ label }}">
                    {{ message }}
                </div>
            {% endfor %}
        {% endfor %}
        {% block body %}{% endblock %}
    </section>
    {# //_______________________________________Footer #}
    {% include "main/incl/footer.html.twig" %}
    </body>
    

</html>